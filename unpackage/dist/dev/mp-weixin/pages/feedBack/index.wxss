.feedbackWrap {
  width: 100%;
  height: 100%;
  min-height: 100%;
  background: #F7F7F7;
}
.feedbackWrap .descr {
  width: 100%;
  height: 500rpx;
}
.feedbackWrap .descr .text {
  width: 100%;
  height: 95%;
  background-color: #FFF;
  position: relative;
}
.feedbackWrap .descr .text .itemWrap {
  width: 95%;
  height: 85%;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.feedbackWrap .upLoadPic {
  width: 100%;
  min-height: 350rpx;
}
.feedbackWrap .upLoadPic .imgWrap {
  width: 100%;
  height: 332rpx;
  background-color: #FFF;
  position: relative;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap {
  width: 95%;
  height: 85%;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .title {
  width: 100%;
  height: 30%;
  line-height: 85rpx;
  color: #434343;
  font-size: 44rpx;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button {
  width: 100%;
  height: 70%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .imgShow {
  width: 200rpx;
  height: 200rpx;
  margin-right: 10rpx;
  position: relative;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .imgShow image {
  width: 100%;
  height: 100%;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .imgShow .iconRed {
  position: absolute;
  right: -12rpx;
  top: -18rpx;
  z-index: 999;
  width: 40rpx;
  height: 40rpx;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .imgShow .iconRed .icon-chacha {
  font-size: 44rpx;
  color: red;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .border {
  width: 200rpx;
  height: 200rpx;
  border: 1rpx dashed #EDEDED;
  position: relative;
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .border .icon {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.feedbackWrap .upLoadPic .imgWrap .positWrap .button .border .icon .icon-shizi-copy {
  font-size: 60rpx;
  color: #C1C1C1;
}
.feedbackWrap .contact {
  width: 100%;
  height: 550rpx;
}
.feedbackWrap .contact .phoneText {
  width: 100%;
  height: 40%;
  background-color: #FFF;
  position: relative;
}
.feedbackWrap .contact .phoneText .positWrap {
  width: 95%;
  height: 85%;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.feedbackWrap .contact .phoneText .positWrap .title {
  width: 100%;
  height: 50%;
  line-height: 94rpx;
  color: #434343;
  font-size: 44rpx;
}
.feedbackWrap .contact .phoneText .positWrap .textar {
  width: 100%;
  height: 50%;
  position: relative;
}
.feedbackWrap .contact .phoneText .positWrap .textar .itemWrap {
  width: 100%;
  height: 55%;
  line-height: 25rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.feedbackWrap .submitButton {
  position: fixed;
  width: 97%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #FEC284;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  bottom: 10rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.feedbackWrap .submitButton .text {
  text-align: center;
  color: #FCDFBB;
}
.feedbackWrap .getPush {
  background-color: orange;
}
.feedbackWrap .getPush .text {
  color: #FFF;
}

